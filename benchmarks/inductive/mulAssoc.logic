theory
  zero	: nat.
  s	: nat ↦ nat.
  +	: (nat, nat) ↦ nat.
  *	: (nat, nat) ↦ nat.
  inductive s.
  inductive zero.
  ∀ y: nat. (zero() + y) = y.
  ∀ x: nat, y: nat. (s(x) + y) = s((x + y)).
  ∀ y: nat. (zero() * y) = zero().
  ∀ x: nat, y: nat. (s(x) * y) = (y + (x * y)).
conjecture
  ∀ x: nat, y: nat, z: nat. (x * (y * z)) = ((x * y) * z).
