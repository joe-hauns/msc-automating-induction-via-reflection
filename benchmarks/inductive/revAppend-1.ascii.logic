theory
  zero	: nat.
  s	: nat -> nat.
  nil	: lst.
  cons	: (nat, lst) -> lst.
  ++	: (lst, lst) -> lst.
  rev	: lst -> lst.
  inductive s.
  inductive zero.
  inductive cons.
  inductive nil.
  forall r: lst. (nil() ++ r) = r.
  forall a: nat, l: lst, r: lst. (cons(a,l) ++ r) = cons(a,(l ++ r)).
  rev(nil()) = nil().
  forall x: nat, xs: lst. rev(cons(x,xs)) = (rev(xs) ++ cons(x,nil())).
conjecture
  forall x: lst. rev((x ++ (x ++ x))) = rev(((x ++ x) ++ x)).
