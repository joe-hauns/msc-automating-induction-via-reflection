theory
  zero	: nat.
  s	: nat -> nat.
  +	: (nat, nat) -> nat.
  *	: (nat, nat) -> nat.
  inductive s.
  inductive zero.
  forall y: nat. (zero() + y) = y.
  forall x: nat, y: nat. (s(x) + y) = s((x + y)).
  forall y: nat. (zero() * y) = zero().
  forall x: nat, y: nat. (s(x) * y) = (y + (x * y)).
conjecture
  forall x: nat, y: nat, z: nat. ((y + z) * x) = ((y * x) + (z * x)).
