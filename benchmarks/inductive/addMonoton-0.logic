theory
  zero	: nat.
  s	: nat ↦ nat.
  +	: (nat, nat) ↦ nat.
  <=	: P(nat, nat).
  inductive s.
  inductive zero.
  ∀ x: nat. (x <= x).
  ∀ x: nat, y: nat. ((x <= y) → (x <= s(y))).
  ∀ y: nat. (zero() + y) = y.
  ∀ x: nat, y: nat. (s(x) + y) = s((x + y)).
conjecture
  ∀ x: nat, y: nat. (x <= (x + y)).
